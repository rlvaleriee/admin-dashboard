{
  "rewrites": [
    { "source": "/((?!api|MediConnect.apk|.*\\..*).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/MediConnect.apk",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"MediConnect.apk\""
        },
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        }
      ]
    }
  ]
}
